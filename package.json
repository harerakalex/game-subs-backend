{
  "name": "games-subscription",
  "version": "1.0.0",
  "description": "game subscription",
  "main": "index.js",
  "repository": "https://github.com/harerakalex/game-subs-backend.git",
  "author": "Harera Kalex",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "build": "tsc",
    "start": "node .",
    "dev": "nodemon",
    "sequelize": "node_modules/.bin/sequelize",
    "db:create:db": "yarn sequelize db:create",
    "db:drop:db": "yarn sequelize db:drop",
    "db:migrate": "yarn sequelize db:migrate",
    "db:migrate:undo": "yarn sequelize db:migrate:undo",
    "db:migrate:rollback": "yarn sequelize db:migrate:undo:all",
    "db:seed": "yarn sequelize db:seed:all",
    "db:seed:undo": "yarn sequelize db:seed:undo",
    "db:seed:undo:all": "yarn sequelize db:seed:undo:all"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.6.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.5.0",
    "sequelize-cli": "^6.2.0",
    "sequelize-typescript": "^2.1.0",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.32",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.3"
  }
}
